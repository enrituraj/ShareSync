

<div class="container">
  <div class="form_body">
      <div class="form_box">
          <div class="form_header">
          <h1>Uploaded Files</h1>
          <a href="/" class="btn btn-view"> Upload</a>
          </div>
          <div class="main_form">
            
            <ul class="file_list">
              <% files.forEach(file => { %>
                <li class="file">
                  <% const ext = file.split("."); %>
                  <%= file.substring(0, 5) + "***" + file.substring(ext[0].length - 3) %>
                  <a class="btn" href="/download/<%= file %>">Download</a>
                </li>
              <% }); %>
            </ul>
            </div>
      </div>
  </div>
</div>

